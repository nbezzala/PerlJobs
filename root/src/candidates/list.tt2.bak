    [% META title = 'Candidate List' -%]
    
    <table>
    <tr>
	<th>ID</th>
	<th>Person ID</th>
	<th>Type</th>
	<th>Job Title</th>
	<th>First Name</th>
	<th>Last Name</th>
    </tr>
    [% # Display each candidate in a table row %]
    [% FOREACH candidate IN candidates -%]
      <tr>
        <td>[% candidate.id %]</td>
	<td>[% candidate.person_id %]</td>
        <td>[% candidate.type %]</td>
        <td>[% candidate.job_title %]</td>
      </tr>
    [% END -%]
    </table>